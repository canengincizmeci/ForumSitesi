
@{
    Layout = null;
}




<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <title>Ekle</title>
</head>
<body>
    <div class="container mt-5">
        <h2 class="mb-4">Kayıt Formu</h2>
        <form method="post" action="/Kullanici/Ekle">
            <div class="mb-3">
                <label for="ad" class="form-label">Ad Soyad</label>
                <input type="text" class="form-control" id="ad" name="ad">
            </div>
            <div class="mb-3">
                <label for="mail" class="form-label">Mail Adresi</label>
                <input type="text" class="form-control" id="mail" name="mail">
            </div>
            
            <button type="submit" class="btn btn-primary">Üye Ekle</button>
        </form>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>
